/*
  Python 3 Grammar
  Based on grammar from Python Language Reference[1] and Python3.g4[2]
  [1] http://docs.python.jp/3.3/reference/index.html
  [2] https://github.com/antlr/grammars-v4/blob/master/python3/Python3.g4
  Author:
    Shun Honda (https://github.com/shun-honda)
*/
File                                = {( NEWLINE / EOS / $(Statement) )* #Source } EOF
Indent                              = <match Indent> S+
NEWLINE                             = '\r'? '\n'
                                    / '\r'
S                                   = [ \t]
_                                   = S*
__                                  = ( S / NEWLINE / '\\' NEWLINE / '#' ( !'\n' . )* )*
___                                 = ( S / '\\' NEWLINE / COMMENT )*
____                                = ( S / NEWLINE / '\\' NEWLINE / COMMENT )+
COMMENT                             = '#' ( !'\n' . )*
EOS                                 = ';'
                                    / NEWLINE
                                    / '#' ( !'\n' . )*
EOF                                 = !.

example COMMENT '''
# comment
'''

// KeyWord
"False"                             = 'False' !W
"class"                             = 'class' !W
"finally"                           = 'finally' !W
"is"                                = 'is' !W
"return"                            = 'return' !W
"None"                              = 'None' !W
"continue"                          = 'continue' !W
"for"                               = 'for' !W
"lambda"                            = 'lambda' !W
"try"                               = 'try' !W
"True"                              = 'True' !W
"def"                               = 'def' !W
"from"                              = 'from' !W
"nonlocal"                          = 'nonlocal' !W
"while"                             = 'while' !W
"and"                               = 'and' !W
"del"                               = 'del' !W
"global"                            = 'global' !W
"not"                               = 'not' !W
"with"                              = 'with' !W
"assert"                            = 'assert' !W
"as"                                = 'as' !W
"elif"                              = 'elif' !W
"if"                                = 'if' !W
"or"                                = 'or' !W
"yield"                             = 'yield' !W
"else"                              = 'else' !W
"import"                            = 'import' !W
"pass"                              = 'pass' !W
"break"                             = 'break' !W
"except"                            = 'except' !W
"in"                                = 'in' !W
"raise"                             = 'raise' !W

// ====================
// Def
// ====================

// function definition
FuncDef                             = { 'def' ___ $name(Name) ___ $param(Parameters) $({ ( _ '->' _ $(Test) )? #TODO }) ___ ':' ___ $body(Block) #FuncDecl }
Parameters                          = '(' __ {(ParameterList)? #List } __ ')'
ParameterList                       = $({ '**' _ $(Parameter) #TODO })
                                    / $({ '*' ( _ $(Parameter) )? ( ___ ',' __ $(Parameter) )* ( ___ ',' __ '**' _ $(Parameter) )? #TODO })
                                    / ( $(DefParameter) ( ___ ',' __ $(DefParameter) )* )? ( ___ ',' __ ('**' _ $(Parameter) / '*' ( _ $(Parameter) )? ( ___ ',' __ $(Parameter) )* ( ___ ',' __ '**' _ $(Parameter) )?) )? ( ___ ',' )?
DefParameter                        = Parameter {$ __ '=' __ $(Test) #Initialize }?
Parameter                           = { $name(Name) {$ __ ':' __ $(Test) #KeyValue }? #Param }
VarArgList                          = { '**' _ $(VarArg) #VarArgs }
                                    / { '*' ( _ $(VarArg) )? ( ___ ',' __ $(VarArg) )* ( ___ ',' __ '**' _ $(VarArg) )? #VarArgs }
                                    / { ( $(DefVarArg) ( ___ ',' __ $(DefVarArg) )* )? ( ___ ',' __ ('**' _ $(VarArg) / '*' ( _ $(VarArg) )? ( ___ ',' __ $(VarArg) )* ( ___ ',' __ '**' _ $(VarArg) )?) )? ( ___ ',' )? #VarArgs }
DefVarArg                           = VarArg {$ __ '=' __ $(Test) #Initialize }?
VarArg                              = Name

// class definition
ClassDef                            = { 'class' ___ $name(Name) ___ ( '(' __ $super(ArgList) __ ')' ___ )? ':' ___ $body(Block) #ClassDecl }
ArgList                             = { '**' _ $(Test) #List }
                                    / { '*' ( _ $(Test) )? ( ___ ',' __ Argument )* ( ___ ',' __ '**' _ $(Test) )? #List }
                                    / { ( Argument ( ___ ',' __ Argument )* )? ( ___ ',' __ ('**' _ $(Test) / '*' ( _ $(Test) )? ( ___ ',' __ Argument )* ( ___ ',' __ '**' _ $(Test) )?) )? ( ___ ',' )? #List }
Argument                            = $({ $(Test) ( __ $(CompFor) )? ___ '=' __ $(Test) #Initialize })
                                    / $(Test) ( __ $(CompFor) )?

example FuncDef '''
def f() :
	pass
'''

example FuncDef '''
def fib(n):
  if n == 1 or n == 2:
    return 1
  else:
    return fib(n-1) + fib(n-2)
'''

example FuncDef '''
def add(x, y):
  return x+y
'''

example ClassDef '''
class Foo:
  pass
'''

example ClassDef '''
class Foo(object):
    pass
'''

// ====================
//      Decorator
// ====================
Decorated                           = { $(Decorators) _ ($(ClassDef) / $(FuncDef)) #Decorated }
Decorators                          = Decorator {$ __ $(Decorator)+ #List }?
Decorator                           = { '@' $(DottedName) ( ___ '(' __ $(ArgList)? __ ')' )? _ NEWLINE #Decorator }

example Decorated '''
@f1(arg)
@f2
class Foo: pass
'''

// ====================
//      Statement
// ====================
Statement                           = SimpleStatement
                                    / CompoundStatement
SimpleStatement                     = SmallStatement {$ ( _ ';' __ $(SmallStatement) )+ #List }? _ ';'? ( '#' ( !'\n' . )* )? (NEWLINE / !.)
SmallStatement                      = ExprStatement
                                    / DelStatement
                                    / PassStatement
                                    / FlowStatement
                                    / ImportStatement
                                    / GlobalStatement
                                    / NonLocalStatement
                                    / AssertStatement
DelStatement                        = { 'del' ___ $(TestList) #Delete }
PassStatement                       = { 'pass' #Pass }
FlowStatement                       = BreakStatement
                                    / ContinueStatement
                                    / ReturnStatement
                                    / RaiseStatement
                                    / YieldStatement
BreakStatement                      = { 'break' #Break }
ContinueStatement                   = { 'continue' #Continue }
ReturnStatement                     = { 'return' ( ___ $expr(TestList) )? #Return }
RaiseStatement                      = { 'raise' ( ___ $(TestList) ( ___ $({ 'from' ___ $(Test) #From }) )? )? #Raise }
YieldStatement                      = YieldExpr
ImportStatement                     = ImportName
                                    / ImportFrom
ImportName                          = { 'import' ___ $(DottedAsNames) #Import }
ImportFrom                          = { $({ 'from' ___ ('.'* $(Module) / '.'+) #From }) ___ $({ 'import' ___ ('(' __ $(ImportList) ( _ ',' )? __ ')' / $(ImportList) / '*') #Import }) #FromImport }

// { @{'from' ___ @Module #From } ___ @{'import' ___ '*' #Import } #FromImport }
Module                              = { ( $(Name) '.' )* $(Name) #Field }
ImportList                          = ImportAsName {$ ( __ ',' __ $(ImportAsName) )+ #List }?
ImportAsName                        = Name {$ ___ 'as' ___ $(Name) #As }?
DottedAsNames                       = DottedAsName {$ ( __ ',' __ $(DottedAsName) )+ #List }?
DottedAsName                        = DottedName {$ ___ 'as' ___ $(Name) #As }?
DottedName                          = Name {$ ( ___ '.' ___ $(Name) )+ #Field }?
GlobalStatement                     = { 'global' ___ $(NameList) #Global }
NonLocalStatement                   = { 'nonlocal' ___ $(NameList) #NonLocal }
NameList                            = { $(Name) ( __ ',' __ $(Name) )* #List }
AssertStatement                     = { 'assert' ___ $(AssertTest) #Assert }
AssertTest                          = Test {$ __ ',' __ $(Test) #List }?

example DelStatement del a
example DelStatement del a[0]
example PassStatement pass
example ReturnStatement return None
example RaiseStatement  raise SpamIndexError(original) from original
example ImportStatement import sound.effects.echo
example ImportStatement from sound.effects import *
example ImportStatement from foo.bar import baz
example ImportStatement import sound.effects.echo as echo
example GlobalStatement global var
example NonLocalStatement nonlocal var
example AssertStatement assert 1 < 2
example NameList a, b

CompoundStatement                   = IfStatement
                                    / WhileStatement
                                    / ForStatement
                                    / TryStatement
                                    / WithStatement
                                    / FuncDef
                                    / ClassDef
                                    / Decorated

// if/elif/else
IfStatement                         = { 'if' ___ $cond(Test) ___ ':' ___ $then(Block) ( $else(Elif) / Else )? #If }
Elif                                = <match Indent> { 'elif' ___ $cond(Test) ___ ':' ___ $then(Block) ($else(Elif) / Else )?  #If }
Else                                = <match Indent> 'else' ___ ':' ___ $else(Block)

// while
WhileStatement                      = { 'while' ___ $cond(Test) ___ ':' ___ $body(Block) Else? #While }

// for
ForStatement                        = { 'for' ___ $(ExprList) ___ 'in' ___ $iter(TestList) ___ ':' ___ $body(Block) Else? #ForEach }

// try/except
TryStatement                        = { 'try' ___ ':' ___ $try(Block) ($catch(ExceptBlock) Else? ( <match Indent> $finally(Finally) )? / <match Indent> $(Finally)) #Try }
ExceptBlock                         = { $(Except)+ #List }
Except                              = <match Indent> { 'except' ( ___ $(TestList) ( ___ $(AsName))?)? ___ ':' ___ $(Block) #Catch }
AsName                              = { 'as' ___ $(Name) #As }
Finally                             = { 'finally' ___ ':' ___ $(Block) #Finally }

// with
WithStatement                       = { 'with' ___ $(WithItemList) ___ ':' ___ $(Block) #With }
WithItemList                        = WithItem {$ ( ___ ',' ___ $(WithItem) )+ #List }?
WithItem                            = Test {$ ___ 'as' ___ $(Expr) #As }?

// Block
Block                               = { $(SimpleStatement) #Block }
                                    / NEWLINE ( _ EOS )* { <block ( <symbol Indent > $(Statement) ( <match Indent> $(Statement) / _ EOS )* )> #Block }

