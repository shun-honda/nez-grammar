example ShiftExpr 1 >> 2
example ArithExpr 1 + 2
example ArithExpr 1 - 2
example Term 1 * 2
example Term 1 / 2
example Term 1 % 2
example Term 1 // 2
example Factor +1
example Factor -1
example Power 1 ** 2
example LambDef lambda x, y: x+y
example Test x if C else y


AtomTrailer                         = Atom {$name ___ addTrailer }*
Atom                                = '(' __ { ( $(YieldExpr) / $(TestListComp) )? #Tuple } __ ')'
                                    / '[' __ { $(TestListComp)? #Array } __ ']'
                                    / '{' __ { $(DictorsetMaker)? #Set } __ '}'
                                    / '...'
                                    / String {$ ( __ $(String) )+ #List }?
                                    / Number
                                    / None
                                    / True
                                    / False
                                    / Name

example Atom [x ** 2 for x in [1, 2, 3, 4, 5]]
example Atom [x ** 2 for x in range(10) if x % 2 == 0]

TestListComp                        = { $(Test) __ ($(CompFor) / ( ',' __ $(Test) __ )* __ ','?) #List }
addTrailer                          = '(' __ $param(ArgList)? __ ')' #Apply
                                    / '[' __ $(SubScriptList) __ ']' #Array
                                    / '.' ___ $(Name) #Field
SubScriptList                       = SubScript {$ ( ___ ',' __ $(SubScript) )+ #List }? ___ ','?
SubScript                           = { $(Test)? ___ ':' ___ $(Test)? ___ addSliceOp? #KeyValue }
                                    / Test
addSliceOp                          = ':' $(Test)?
ExprList                            = { $(StarExpr) ( __ ',' __ $(StarExpr) )* __ ','? #List }
DictorsetMaker                      = { $(Test) ___ (':' __ $(Test) ___ ($(CompFor) / ( ',' __ $(Test) ___ ':' ___ $(Test) ___ )* ','?) / $(CompFor) / ( ',' __ $(Test) __ )* ','?) #Set }
CompIter                            = CompFor
                                    / CompIf
CompFor                             = { 'for' ___ $(ExprList) ___ 'in' ___ $(OrTest) !( _ ',' ) !( _ ':' ) ( __ $(CompIter) )? #For }
CompIf                              = { 'if' ___ $(TestNoCond) ( __ $(CompIter) )? #If }
YieldExpr                           = { 'yield' ___ $(YieldArg)? #Yield }
YieldArg                            = { 'from' ___ $(Test) #From }
                                    / TestList
//NoParenthesesArgumentList_FuncCall  = Name {$ _ !( KEYWORD S+ / '(' / ',' ) Argument #Apply }
None                                = { 'None' #None }
True                                = { 'True' #True }
False                               = { 'False' #False }
Name                                = { NAME #Name }
NAME                                = !(KEYWORD) ID_START ID_CONTINUE*
W                                   = [a-zA-Z0-9_]
KEYWORD                             = "False"
                                    / "class"
                                    / "finally"
                                    / "is"
                                    / "return"
                                    / "None"
                                    / "continue"
                                    / "for"
                                    / "lambda"
                                    / "try"
                                    / "True"
                                    / "def"
                                    / "from"
                                    / "nonlocal"
                                    / "while"
                                    / "and"
                                    / "del"
                                    / "global"
                                    / "not"
                                    / "with"
                                    / "assert"
                                    / "as"
                                    / "elif"
                                    / "if"
                                    / "or"
                                    / "yield"
                                    / "else"
                                    / "import"
                                    / "pass"
                                    / "break"
                                    / "except"
                                    / "in"
                                    / "raise"
Number                              = ImagNumber
                                    / FloatNumber
                                    / Integer
Integer                             = { (OCT_INTEGER / HEX_INTEGER / BIN_INTEGER / DECIMAL_INTEGER) [lL]? #Integer }
DECIMAL_INTEGER                     = DIGIT+
                                    / '0'+
OCT_INTEGER                         = '0' [oO] OCT_DIGIT+
HEX_INTEGER                         = '0' [xX] HEX_DIGIT+
BIN_INTEGER                         = '0' [bB] BIN_DIGIT+
FloatNumber                         = { (POINT_FLOAT / EXPONENT_FLOAT) #Float }
ImagNumber                          = { ($(FloatNumber) / INT_PART) [jJ] #Imaginary }
NON_ZERO_DIGIT                      = [1-9]
DIGIT                               = [0-9]
OCT_DIGIT                           = [0-7]
HEX_DIGIT                           = [0-9a-fA-F]
BIN_DIGIT                           = [01]
POINT_FLOAT                         = INT_PART? FRACTION
                                    / INT_PART '.'
EXPONENT_FLOAT                      = (INT_PART / POINT_FLOAT) EXPONENT
INT_PART                            = DIGIT+
FRACTION                            = '.' DIGIT+
EXPONENT                            = [eE] [+-]? DIGIT+
String                              = StringLiteral
                                    / BytesLiteral
StringLiteral                       = [uU]? [rR]? (ShortString / LongString)
BytesLiteral                        = [bB] [rR]? (ShortBytes / LongBytes)
ShortString                         = !'\'\'\'' '\'' { ( STRING_ESCAPE_SEQ / ![\\\r\n'] . )* #String } '\''
                                    / !'"""' '"' { ( STRING_ESCAPE_SEQ / ![\\\r\n"] . )* #String } '"'
LongString                          = '\'\'\'' { ( STRING_ESCAPE_SEQ / ![\\'] . / !'\'\'\'' '\'' )* #String } '\'\'\''
                                    / '"""' { ( STRING_ESCAPE_SEQ / ![\\"] . / !'"""' '"' )* #String } '"""'
STRING_ESCAPE_SEQ                   = '\\' .
ShortBytes                          = !'\'\'\'' '\'' { ( SHORT_BYTES_CHAR_NO_SINGLE_QUOTE / BYTES_ESCAPE_SEQ )* #Bytes } '\''
                                    / !'"""' '"' { ( SHORT_BYTES_CHAR_NO_DOUBLE_QUOTE / BYTES_ESCAPE_SEQ )* #Bytes } '"'
LongBytes                           = '\'\'\'' { LONG_BYTES_ITEM* #Bytes } '\'\'\''
                                    / '"""' { LONG_BYTES_ITEM* #Bytes } '"""'
LONG_BYTES_ITEM                     = LONG_BYTES_CHAR
                                    / BYTES_ESCAPE_SEQ
SHORT_BYTES_CHAR_NO_SINGLE_QUOTE    = [\u0000-\u0009\u000B-\u000C\u000E-\u0026\u0028-\u005B\u005D-\u007F]
SHORT_BYTES_CHAR_NO_DOUBLE_QUOTE    = [\u0000-\u0009\u000B-\u000C\u000E-\u0021\u0023-\u005B\u005D-\u007F]
LONG_BYTES_CHAR                     = [\u0000-\u005B\u005D-\u007F]
