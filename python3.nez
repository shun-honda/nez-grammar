// ====================
//      Expression
// ====================
ExprStatement                       = TestListStarExpr {$left ___ addAssignOp ___ ($right(YieldExpr) / $right(TestList)) }
                                    / TestListStarExpr {$name ___ '=' __ ($expr(YieldExpr) / $expr(TestListStarExpr)) #VarDecl }*
addAssignOp                         = '+=' #AssignAdd
                                    / '-=' #AssignSub
                                    / '**=' #AssignPow
                                    / '//=' #AssignFloorDiv
                                    / '*=' #AssignMul
                                    / '/=' #AssignDiv
                                    / '%=' #AssignMod
                                    / '&=' #AssignBitwiseAnd
                                    / '|=' #AssignBitwiseOr
                                    / '^=' #AssignBitwiseXOr
                                    / '<<=' #AssignLeftShift
                                    / '>>=' #AssignRightShift
TestListStarExpr                    = TestStarExpr {$ ___ ',' __ $(TestStarExpr) #List }* ( ___ ',' )?
TestStarExpr                        = Test
                                    / StarExpr
TestList                            = { $(Test) ( ___ ',' __ $(Test) )* #List } ( ___ ',' )?
Test                                = OrTest {$ ___ 'if' ___ $(OrTest) ___ 'else' ___ $(Test) #If }?
                                    / LambDef
TestNoCond                          = OrTest
                                    / LambDefNoCond
LambDef                             = { 'lambda' ___ $(VarArgList)? ___ ':' __ $(Test) #Lambda }
LambDefNoCond                       = { 'lambda' ___ $(VarArgList)? ___ ':' __ $(TestNoCond) #Lambda }
OrTest                              = AndTest {$left __ 'or' ____ $right(AndTest) #Or }*
AndTest                             = NotTest {$left __ 'and' ____ $right(NotTest) #And }*
NotTest                             = { 'not' ____ $expr(NotTest) #Not }
                                    / Comparison
Comparison                          = StarExpr {$left ___ addCompOp __ $right(StarExpr) }*
addCompOp                           = '>=' #GreaterThanEquals
                                    / '<=' #LessThanEquals
                                    / '<' !'<' #LessThan
                                    / '>' !'>' #GreaterThan
                                    / '==' #Equals
                                    / '!=' #NotEquals
                                    / 'in' #In
                                    / 'not' __ 'in' #NotIn
                                    / 'is' __ 'not' #IsNot
                                    / 'is' #Is
StarExpr                            = '*'? Expr
Expr                                = XOrExpr {$left __ '|' !'|' __ $right(XOrExpr) #BitwiseOr }*
XOrExpr                             = AndExpr {$left __ '^' __ $right(AndExpr) #BitwiseXor }*
AndExpr                             = ShiftExpr {$left __ '&' !'&' __ $right(ShiftExpr) #BitwiseAnd }*
ShiftExpr                           = ArithExpr {$left __ ('<<' #LeftShift / '>>' #RightShift) __ $right(ArithExpr) }*
ArithExpr                           = Term {$left __ ('+' #Add / '-' #Sub) __ $right(Term) }*
Term                                = Factor {$left __ ('*' #Mul / '%' #Mod / '//' #FloorDiv / '/' #Div) __ $right(Factor) }*
Factor                              = { ('+' #Plus / '-' #Minus / '~' #Compl) __ $expr(Factor) }
                                    / Power
Power                               = AtomTrailer {$left __ '**' __ $right(Factor) #Pow }?

